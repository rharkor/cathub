{
  "name": "@cathub/api-routes",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "lint": "eslint",
    "lint-report": "eslint . -f ../../node_modules/eslint-detailed-reporter/lib/detailed.js -o ../../report/report-packages.html",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/lib/constants.d.ts",
      "default": "./dist/lib/constants.js"
    },
    "./utils": {
      "types": "./dist/lib/utils.d.ts",
      "default": "./dist/lib/utils.js"
    },
    "./types": {
      "types": "./dist/lib/types.d.ts",
      "default": "./dist/lib/types.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "default": "./dist/schemas.js"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.740.0",
    "@aws-sdk/s3-presigned-post": "^3.740.0",
    "@cathub/database-main": "*",
    "@prisma/client": "^6.3.1",
    "@rharkor/logger": "^1.3.1",
    "@t3-oss/env-core": "^0.12.0",
    "@trpc/server": "npm:@trpc/server@next",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.9.0",
    "@types/qs": "^6.9.18",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsonwebtoken": "^9.0.2",
    "pkgroll": "^2.8.0",
    "prisma": "^6.3.1",
    "superjson": "^2.2.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "zod": "^3.24.1"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6"
  }
}
