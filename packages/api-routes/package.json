{
  "name": "@cathub/api-routes",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "pkgroll",
    "build:dev": "pkgroll --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "dev": "pkgroll --watch"
  },
  "keywords": [],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./constants": {
      "require": {
        "types": "./dist/lib/constants.d.cts",
        "default": "./dist/lib/constants.cjs"
      },
      "import": {
        "types": "./dist/lib/constants.d.mts",
        "default": "./dist/lib/constants.mjs"
      }
    },
    "./utils": {
      "require": {
        "types": "./dist/lib/utils.d.cts",
        "default": "./dist/lib/utils.cjs"
      },
      "import": {
        "types": "./dist/lib/utils.d.mts",
        "default": "./dist/lib/utils.mjs"
      }
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.740.0",
    "@aws-sdk/s3-presigned-post": "^3.740.0",
    "@prisma/client": "^6.2.1",
    "@rharkor/logger": "^1.3.1",
    "@t3-oss/env-core": "^0.12.0",
    "@trpc/server": "npm:@trpc/server@next",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/node": "^22.9.0",
    "@types/qs": "^6.9.18",
    "dotenv": "^16.4.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "pkgroll": "^2.6.1",
    "superjson": "^2.2.2",
    "typescript-eslint": "^8.22.0",
    "zod": "^3.24.1"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "prisma": {
    "schema": "./prisma/schema"
  },
  "devDependencies": {
    "eslint": "^9.19.0",
    "prisma": "^6.2.1",
    "typescript": "^5.7.3"
  }
}