{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:fix": {
      "cache": false
    },
    "prettier": {},
    "prettier:fix": {
      "cache": false
    },
    "format": {
      "dependsOn": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false
    }
  }
}